select "std"


arr = [1,7,3,9,15] 
insertionSort(arr)

func insertionSort(arr){ 
  for(i=1,i<length(arr),i++)
  {
	key = arr[i] 
	j = i-1
	
	while( j >= 0 && key < arr[j]) 
	{
			arr[j + 1] = arr[j] 
			j -= 1
	}
    arr[j + 1] = key 
		
  }
  
  for(i=0,i<length(arr),i++)
  {
	showln arr[i]
  }
 }

 

	
	
